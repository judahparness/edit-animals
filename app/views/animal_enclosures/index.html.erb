<p id="notice"><%= notice %></p>

<h1>Listing Animal Enclosures</h1>

<table>
  <thead>
    <tr>
      <th>Animal</th>
      <th>Enclosure</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @animal_enclosures.each do |animal_enclosure| %>
      <tr>
        <td><%= animal_enclosure.animal %></td>
        <td><%= animal_enclosure.enclosure %></td>
        <td><%= link_to 'Show', animal_enclosure %></td>
        <td><%= link_to 'Edit', edit_animal_enclosure_path(animal_enclosure) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Animal enclosure', new_animal_enclosure_path %>
